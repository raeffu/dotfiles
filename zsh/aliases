# diverses
alias cls='clear'
alias dhcp-renew='sudo ipconfig set en0 BOOTP;sudo ipconfig set en0 DHCP'

# cd
alias ..='cd ..'

# ls
alias ls="ls -Gh"
alias l="ls -lAh"
alias ll="ls -l"
alias la='ls -A'

# git
alias gl='git pull'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status'
alias gsm='git submodule init'
alias gr='git remote'
alias gf='git fetch'

# commands starting with % or $ for pasting from web
alias %=' ' 
alias \$=' '
